    BackgroundColor="#D5D6D2"
    Title="Login">

    <ContentPage.Content>
        <AbsoluteLayout >
             <Image Source="rassap_bw" AbsoluteLayout.LayoutBounds="110,80,240,240" AbsoluteLayout.LayoutFlags="None" Aspect="AspectFit" Opacity="0.1"/>
